<h1>Lista de Competidores</h1>

<table frame="border" border="4" >
  <thead>
    <tr>
      <th>ID del competidor</th>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Date admission</th>
      <th>Email</th>
      <th>Mensaje</th>
      <th>Estado video</th>
      <th>Url video original</th>
      <th>Url video converted</th>
      <th>Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @competitors.each do |competitor| %>
      <tr>
        <td><%= competitor.id %></td>
        <td><%= competitor.first_name %> <%= competitor.second_name %></td>
        <td><%= competitor.last_name %> <%= competitor.second_last_name %></td>
        <td><%= competitor.date_admission %></td>
        <td><%= competitor.email %></td>
        <td><%= competitor.message %></td>
        <td><%= competitor.status_video %></td>
        <td><%= competitor.video_original %></td>
        <td><%= competitor.video_converted %></td>
        <td><%= link_to 'Ver', competitor %>|
        <%= link_to 'Editar', edit_competitor_path(competitor) %>|
        <%= link_to 'Eliminar', competitor, method: :delete, data: { confirm: 'Esta seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @competitors %>
<br>
